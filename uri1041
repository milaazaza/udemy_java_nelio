import java.util.Scanner;

public class uri1041 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		double x = sc.nextDouble();
		double y = sc.nextDouble();

		String cord;
		if (x > 0 && y > 0) {
			cord = "Q1";

		} else if (x > 0 && y < 0) {
			cord = "Q2";
		} else if (x < 0 && y < 0) {
			cord = "Q3";
		} else if (x < 0 && y > 0) {
			cord = "Q4";
		} else {

			cord = "origem";

		}
		System.out.println(cord);
		sc.close();

	}

}
